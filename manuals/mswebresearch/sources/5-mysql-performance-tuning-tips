						tinyMCE_TelligentEvolutionOptions.SetContent(val, tinyMCE.get('fragment-54581_fragment-54581_createEditor'));
					}
					return (tinyMCE.get('fragment-54581_fragment-54581_createEditor').getContent() || '').replace(/(?:^\W*<p[^>]*>(?:&nbsp;)?<\/p>|<p[^>]*>(?:&nbsp;)?<\/p>\W*$)/gi, '')
				},
				focus: function() {
					try { tinyMCE.get('fragment-54581_fragment-54581_createEditor').plugins.enhancements.focusAndView(); } catch (e) { }
				}
			}
		});
	});
	</script>

