            <div class="clearfix text-formatted field field--name-body field--type-text-with-summary field--label-hidden field__item"><script>
/*
 * Mixpanel event calls
 */

// function executes if user wants to opt out of tracking
function optOut() {
  mixpanel.opt_out_tracking();
}
// function executes if user wants to opt into tracking
function optIn() {
  mixpanel.opt_in_tracking();
}

// function executes when user loads home page
function viewHome() {
  // Send a "viewHome" event to Mixpanel
  // with test property object
  
  mixpanel.track(
    "viewHome",
    {"tracktype": "view home page"}
  );
}
// function executes when user loads login page
function viewLogin() {
  // Send a "viewLogin" event to Mixpanel
  // with test property object
  mixpanel.track(
    "viewLogin",
    {"tracktype": "view login page"}
  );
}
// function executes when user logins successfully
function viewWelcome() {
  // Send a "viewLogin" event to Mixpanel
  // with test property object
  mixpanel.track(
    "viewWelcome",
    {"tracktype": "view viewWelcome page"}
  );
}
// function executes when user loads registration page
function viewRegistration() {
  // Send a "viewRegistration" event to Mixpanel
  // with test property object
  mixpanel.track(
    "viewRegistration",
    {"tracktype": "view registration page"}
  );
}
// function executes when user loads registration page
function viewThankYou() {
  // Send a "viewRegistration" event to Mixpanel
  // with test property object
  mixpanel.track(
    "viewThankYou",
    {"tracktype": "view thank you for signing page"}
  );
}
// function executes when user loads downloads page
function viewDownloads() {
  // Send a "viewDownloads" event to Mixpanel
  // with test property object
  mixpanel.track(
    "viewDownloads",
    {"tracktype": "view downloads page"}
  );
}
// function executes when user loads any other page
function viewSite() {
  // Send a "viewSite" event to Mixpanel
  // with test property object
  mixpanel.track(
    "viewSite",
    {"tracktype": "view any page"}
  );
}

// by default the user has opted in.
var mixpanelNonConsent = mixpanel.has_opted_out_tracking();
if (!mixpanelNonConsent) {
  //TODO place event triggers in here.
}

/*
 * Event Triggers
 */
// page views
document.addEventListener('DOMContentLoaded', function(e) {

  var pageUrl = window.location.href;
  pageUrl = '/' + pageUrl.replace(/^(?:\/\/|[^/]+)*\//, '');

  switch(pageUrl) {
    case '/':
      viewHome();
      break;
    case '/user/login':
      viewLogin();
      break;
    case '/welcome':
      viewWelcome();
      break;
    case '/user/register':
      viewRegistration();
      break;
    case '/thank-you-creating-account':
      viewThankYou();
      break;
    case '/software-downloads-postgres':
      viewDownloads();
      break;
    default:
      viewSite();

  }
});

</script></div>
      
    </div>
  </div>



              </main>
                        
        </div>
          </div>
                  <div class="featured-bottom text-center bg-primary d-flex justify-content-center">
