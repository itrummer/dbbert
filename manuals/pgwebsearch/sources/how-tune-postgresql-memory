            <div class="clearfix text-formatted field field--name-body field--type-text-with-summary field--label-hidden field__item"><script>
/*
 * Mixpanel event calls
 */

// function executes if user wants to opt out of tracking
function optOut() {
  mixpanel.opt_out_tracking();
}
// function executes if user wants to opt into tracking
function optIn() {
  mixpanel.opt_in_tracking();
}

// function executes when user loads home page
function viewHome() {
  // Send a "viewHome" event to Mixpanel
  // with test property object
  
  mixpanel.track(
    "viewHome",
    {"tracktype": "view home page"}
  );
}
// function executes when user loads login page
function viewLogin() {
  // Send a "viewLogin" event to Mixpanel
  // with test property object
  mixpanel.track(
    "viewLogin",
    {"tracktype": "view login page"}
  );
}
// function executes when user logins successfully
function viewWelcome() {
  // Send a "viewLogin" event to Mixpanel
  // with test property object
  mixpanel.track(
    "viewWelcome",
    {"tracktype": "view viewWelcome page"}
  );
}
