// page views
document.addEventListener('DOMContentLoaded', function(e) {

  var pageUrl = window.location.href;
  pageUrl = '/' + pageUrl.replace(/^(?:\/\/|[^/]+)*\//, '');

  switch(pageUrl) {
    case '/':
      viewHome();
      break;
    case '/user/login':
      viewLogin();
      break;
    case '/welcome':
